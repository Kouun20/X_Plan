<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.x_plan">

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.SIGN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity android:name=".RankActivity"  android:screenOrientation="landscape">
            android:screenOrientation="landscape">
            <intent-filter>

                <action android:name="android.intent.action.ALL_APPS" />
            </intent-filter>
        </activity>
        <activity android:name=".HelpActivity"  android:screenOrientation="landscape">
            android:screenOrientation="landscape">
            <intent-filter>

                <action android:name="android.intent.action.ALL_APPS" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>